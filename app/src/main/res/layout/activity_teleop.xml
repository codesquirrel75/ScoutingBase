                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:clickable="false"
                        android:cursorVisible="false"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:enabled="true" />
                    </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/teleop_low_made_input_layout"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    app:errorEnabled="true"
                    tools:ignore="UnknownId">

                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/teleop_low_made_input"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:clickable="false"
                        android:cursorVisible="false"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:enabled="true" />
                </android.support.design.widget.TextInputLayout>


                <Button
                    android:id="@+id/decrease_teleop_low_made"
                    android:layout_width="30pt"
                    android:layout_height="30pt"
                    android:onClick="decreaseLowMadeInput"
                    android:text="@string/minus"/>
                <Button
                    android:id="@+id/increase_teleop_cargo_top"
                    android:layout_width="30pt"
                    android:layout_height="30pt"
                    android:onClick="increaseLowMadeInput"
                    android:text="@string/plus"
                    />




            </LinearLayout>
        </LinearLayout>







